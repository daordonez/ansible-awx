# Ansible AWX - Configuración Básica

[defaults]
# Configuración esencial
host_key_checking = False
roles_path = roles
timeout = 30
gathering = smart

# Gestión de secrets
vault_password_file = vault/.vault_pass

# Logging básico
log_path = logs/ansible.log
display_skipped_hosts = False

[privilege_escalation]
become = False

[ssh_connection]
pipelining = True